    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.kneecheck.ui.dashboard.DashboardFragment"
        tools:layout="@layout/fragment_dashboard_dokter" />

    <fragment
        android:id="@+id/navigation_scan"
        android:name="com.example.kneecheck.ui.scan.ScanFragment"
        tools:layout="@layout/fragment_scan_dokter" />

    <fragment
        android:id="@+id/navigation_histori"
        android:name="com.example.kneecheck.ui.histori.HistoriFragment"
        tools:layout="@layout/fragment_histori_dokter" />

    <fragment
